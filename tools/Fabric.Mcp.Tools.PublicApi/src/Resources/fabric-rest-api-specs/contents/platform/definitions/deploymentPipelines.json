                    "readOnly": true
                },
                "noDifferenceItemsCount": {
                    "type": "integer",
                    "description": "The number of identical deployed items in the source and target stages, before deployment.",
                    "readOnly": true
                }
            },
            "readOnly": true
        },
        "DeploymentPipelineOperation": {
            "required": ["id", "type", "status", "lastUpdatedTime"],
