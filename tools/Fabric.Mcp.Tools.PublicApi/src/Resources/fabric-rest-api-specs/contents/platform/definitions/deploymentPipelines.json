                    "readOnly": false
                },
                "note": {
                    "type": "string",
                    "description": "A note describing the deployment. The text size is limited to 1024 characters.",
                    "maxLength": 1024,
                    "readOnly": false
                },
                "items": {
                    "type": "array",
                    "description": "A list of items to be deployed. If not used, all supported stage items are deployed.",
