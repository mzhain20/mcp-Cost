                },
                "displayName": {
                    "type": "string",
                    "description": "The deployment pipeline display name.",
                    "readOnly": false
                },
                "description": {
                    "type": "string",
                    "description": "The deployment pipeline description.",
                    "readOnly": false
                }
            }
        },
        "DeploymentPipelineExtendedInfo": {
