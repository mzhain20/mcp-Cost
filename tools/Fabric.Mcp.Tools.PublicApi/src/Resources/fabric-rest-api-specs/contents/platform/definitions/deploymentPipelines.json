            "required": ["workspaceId"],
            "description": "A request to assign a workspace to a deployment pipeline stage.",
            "properties": {
                "workspaceId": {
                    "type": "string",
                    "format": "uuid",
                    "description": "The workspace ID.",
                    "readOnly": false
                }
