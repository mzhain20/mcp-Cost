                "id": {
                    "type": "string",
                    "format": "uuid",
                    "description": "The deployment pipeline stage ID.",
                    "readOnly": true
                },
                "order": {
                    "type": "integer",
                    "description": "The stage order, starting from zero.",
                    "readOnly": true
                },
                "displayName": {
                    "type": "string",
                    "description": "The deployment pipeline stage display name.",
                    "readOnly": false
                },
                "description": {
                    "type": "string",
                    "description": "The deployment pipeline stage description.",
                    "readOnly": false
                },
                "workspaceId": {
                    "type": "string",
