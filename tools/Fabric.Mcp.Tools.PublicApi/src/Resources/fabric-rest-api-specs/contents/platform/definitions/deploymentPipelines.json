                "executionEndTime": {
                    "type": "string",
                    "format": "date-time",
                    "description": "The date and time that the operation ended.",
                    "readOnly": true
                },
                "sourceStageId": {
                    "type": "string",
                    "format": "uuid",
                    "description": "The ID of a source deployment pipeline stage.",
                    "readOnly": true
                },
                "targetStageId": {
                    "type": "string",
                    "format": "uuid",
                    "description": "The ID of a target deployment pipeline stage.",
                    "readOnly": true
